#返回函数
def create_counter():
    s = [0]
    
    def counter():
        s[0] += 1
        return s[0]
    return conter
    
    
counterA = create_counter()
print(counterA(),counterA(),counterA(),counterA())


#匿名函数
def is_odd(n):
    return n % 2 == 1

L = list(filter(lambda n:n%2==1, range(1, 20)))
